{
    "keyboard_name": "Open keyboard",
    "manufacturer": "Miguelio",
    "url": "https://miguelio.com",
    "maintainer": "miguelio",
    "tags": ["4keys", "small", "pad"],

    "usb": {
        "vid": "0x0001",
        "pid": "0x0002",
        "device_version": "1.0.0"
    },

    "processor": "atmega32u4",
    "bootloader": "caterina",
    
    "diode_direction": "COL2ROW",
    "matrix_pins": {
        "cols": ["D1", "D0"],
        "rows": ["D4", "C6"]
    },

    "debounce": 20,
    
    "layouts": {
        "LAYOUT_4_keys": {
            "layout": [
                { "matrix": [0, 0], "label":"1", "x":0, "y":0 },
                { "matrix": [1, 0], "label":"2", "x":1, "y":0 },
                { "matrix": [0, 1], "label":"3", "x":0, "y":1 },
                { "matrix": [1, 1], "label":"4", "x":1, "y":1 },
            ]
          }
    }
}
